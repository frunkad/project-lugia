
    .mdc-top-app-bar__section--align-start {
      justify-content: flex-start;
      order: -1; }
    .mdc-top-app-bar__section--align-end {
      justify-content: flex-end;
      order: 1; }
.mdc-top-app-bar__row {
  display: flex;
  width: 100%;
}
.mdc-top-app-bar__section {
  display: -webkit-inline-box;
  display: -ms-inline-flexbox;
  display: inline-flex;
  -webkit-box-flex: 1;
  -ms-flex: 1 1 auto;
  flex: 1 1 auto;
  -webkit-box-align: center;
  -ms-flex-align: center;
  align-items: center;
  min-width: 0;
  padding: 8px 12px;
  z-index: 1;
}
.fill-remaining-space {
    /* This fills the remaining space, by using flexbox. 
       Every toolbar row uses a flexbox row layout. */
    flex: 1 1 auto;
  }
.sidenav-container {
  width: 100%;
  // height: 64px;
}
mat-sidenav {
  width: calc(100% -64px);
  max-width: 300px;
  min-width: 200px;
}

.dialog_body {
  padding: 24px 34px;
}

.signout_dialog h3 {
  font-weight: 500;
  font-size: 22px;
  line-height: 32px;
  margin-bottom: 34px;
  text-align: center;
  color: #536cf6;
}

.signout_dialog h3 {
  text-align: left;
  margin-bottom: 26px;
}

.signout_dialog p {
  line-height: 28px;
}

.dialog_body .btn_container {
  display: flex;
  justify-content: center;
  align-items: center;
  flex-direction: column;
}

.dialog_body .btn_container button {
  cursor: pointer;
  margin-bottom: 12px;
  font-weight: 500;
  height: auto;
  line-height: normal;
  max-width: 250px;
  min-height: 40px;
  padding: 8px 16px;
  text-align: left;
  width: 100%;
  box-shadow: 0 2px 2px 0 rgba(0,0,0,.14), 0 3px 1px -2px rgba(0,0,0,.2), 0 1px 5px 0 rgba(0,0,0,.12);
  background: white;
  border: none;
  border-radius: 4px;
  display: flex;
  align-items: center;
}

.dialog_body .btn_container img {
  width: 18px;
  height: 18px;
}

.dialog_body .btn_container .btn_label {
  font-size: 15px;
  line-height: 15px;
  padding-left: 16px;
  color: white;
  font-weight: 500;
  font-family: 'Google Sans', sans-serif;
}

.dialog_body .btn_container #google .btn_label {
  color: #57575a;
}

.dialog_body .btn_container #facebook {
  background-color: #3b5998;
}

.dialog_body .btn_container #twitter {
  background-color: #55acee;
}

.dialog_body .btn_container #github {
  background-color: #333;
}

.cancel_btn,
.signout_btn {
  cursor: pointer;
  color: #527aee;
  font-weight: 500;
  background: white;
  border: none;
  font-size: 15px;
  text-align: center;
  padding: 12px 8px;
  font-family: 'Google Sans', sans-serif;
  min-width: 100px;
  border-radius: 4px;
}

.signout_btn {
  color: #fff;
  font-weight: 500;
  background: #527aee;
}

.cancel_btn:focus,
.signout_btn:focus {
  outline: none;
}

.toolbar {
  display: none;
}

.menu_icon {
  fill: black;
  padding: 12px;
  width: 48px;
  height: 48px;
  cursor: pointer;
  border-radius: 50%;
}

.desktop_toolbar {
  height: 64px;
  display: flex;
  justify-content: flex-end;
  align-items: center;
  padding: 0 16px;
}

.desktop_toolbar img,
.toolbar img {
  height: 40px;
  width: 40px;
  border-radius: 40px;
  border: 2px solid #536cf6;
  margin-right: 8px;
  cursor: pointer;
}

.desktop_toolbar img {
  height: 42px;
  width: 42px;
  border-radius: 42px;
}

.desktop_toolbar .signin_btn,
.toolbar .signin_btn {
  color: #2a45d8;
  margin-right: 8px;
  font-weight: 500;
  cursor: pointer;
  padding: 12px 8px;
}

.drawer_toolbar {
  font-family: 'Google Sans', sans-serif;
  border-bottom: 1px solid rgb(230, 230, 230);
  margin-bottom: 16px;
  padding: 24px 16px 24px 23px;
}

.drawer_toolbar svg {
  width: 72px;
}

.drawer_toolbar h2 {
  font-weight: 400;
  font-size: 18px;
  line-height: 24px;
  margin: 16px 0px 6px;
}

.drawer_toolbar p {
  font-weight: 400;
  font-size: 15px;
  line-height: 22px;
  margin: 0px;
}

.drawer_nav {
  padding-left: 23px;
}

.drawer_nav a {
  font-family: 'Google Sans', sans-serif;
  font-weight: 400;
  color: #202124;
  display: block;
  font-size: 15px;
  line-height: 22px;
  padding: 10px 0;
  text-decoration: none;
  min-width: 255px;
  cursor: pointer;
  transition: 50ms color ease;
}

.drawer_nav a:hover {
  color: #4768fd;
}

.navbar {
  background: #4768fd;
  bottom: 0px;
  left: 0px;
  position: fixed;
  top: 0px;
  width: 72px;
  z-index: 1;
  transform: translate(0, 0);
  transition: transform 0.75s cubic-bezier(0.190, 1.000, 0.220, 1.000);
}

.navbar .hamburger {
  display: flex;
  justify-content: center;
  left: 0px;
  position: absolute;
  text-align: center;
  top: 20px;
  width: 72px;
}

.navbar .hamburger .menu_icon {
  fill: #fff;
}

.navbar nav {
  left: 0px;
  position: absolute;
  right: 0px;
  top: 50%;
  transform: translateY(-50%);
}

.navbar nav .nav_item {
  height: 95px;
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
  font-size: 11px;
  font-weight: 500;
  text-decoration: none;
  color: white;
  opacity: 0.6;
  border-right: 3px solid transparent;
  transition: border 400ms ease;
}

.navbar nav .nav_item:hover {
  border-right: 3px solid rgba(255, 255, 255, 0.6)
}

.navbar nav .nav_item.active {
  opacity: 1;
  border-right: 3px solid #fff;
}

.navbar nav .nav_item:focus {
  outline: none;
}

.navbar nav .nav_item svg {
  width: 24px;
  height: 24px;
  fill: #fff;
}

.navbar nav .nav_item span {
  margin-top: 12px;
}

@media screen and (max-width: 768px) {
  .dialog_body {
     padding: 16px 28px;
  }
  .toolbar {
      display: block;
  }

  .toolbar .mobile_title {
      color: black;
      position: absolute;
      height: 100%;
      width: 100%;
      display: flex;
      justify-content: center;
      align-items: center;
      font-size: 20px;
      font-weight: 500;
  }

  .desktop_toolbar {
      display: none;
  }

  .navbar {
      bottom: -2px;
      height: 60px;
      right: 0px;
      z-index: 2;
      width: auto;
      top: auto;
  }

  .navbar .hamburger {
      display: none;
  }

  .navbar nav {
      position: relative;
      display: flex;
      justify-content: space-around;
  }

  .navbar nav .nav_item {
      height: 60px;
      width: 100%;
  }

  .navbar nav .nav_item,
  .navbar nav .nav_item:hover,
  .navbar nav .nav_item.active {
      border-right: none;
  }

  .navbar nav .nav_item span {
      margin-top: 6px;
  }
}

